import java.util.Scanner;

public class probando {

	public static void main(String[] args) {

		Scanner teclado=new Scanner(System.in);
	
		
	int numero=0;
	int factorial=0;
	
		int j;
		for(int i=2;i<=10;i++){
			
			for(j=i-1;j>0;j--){
				factorial=numero*j;
				
			}
			
			System.out.println("El factorial del numero "+ numero + " es el numero " + factorial);
		}
		
		
	}
}
